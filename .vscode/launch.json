{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDB Debug (Remote)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/freertos_project.elf",
            "miDebuggerPath": "/usr/bin/gdb-multiarch",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set target remote",
                    "text": "target remote :3333",
                    "ignoreFailures": false
                }
            ],
            "preLaunchTask": "Start GDB Server",
            "postDebugTask": "Stop GDB Server"
        }
    ]
}
